@using IpCamMonitor.Models.HomeModel
@model List<HomeViewModel>
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <link href="~/Content/bootstrap.css" rel="stylesheet" />
    <link href="~/Content/bootstrap-theme.css" rel="stylesheet" />
    <title>Index</title>
</head>
<body>
<div id="main_panel">
    <div class="text-center">
        <h1>ООРКК - Онлайн мониторинг аудиторий</h1>
    </div class="btn btn-success" >
        @Html.ActionLink("Настройки", "/Index/", "Settings")
        @foreach(var pic in Model)
        {
            <div class="cam-port">
                <h3 class="text-center">@pic.Title</h3>
                <img src="@pic.FileName" width="304" height="236" class="img-thumbnail" />
            </div>
        }



    </div>
    
</body>
</html>
